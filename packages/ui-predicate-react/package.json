{
  "name": "ui-predicate-react",
  "version": "0.2.0",
  "description": "Finally a predicate UI component for the Web, for React",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo 'not ready for prime-time';true",
    "clean": "rm -rf lib && mkdir lib",
    "lib": "npm run clean && NODE_ENV=production babel src -d lib",
    "prepublish": "npm run lib",
    "test:debugger": "node --inspect-brk node_modules/.bin/jest --watch",
    "test:watch": "jest --watch --coverage=false",
    "test:watch:coverage": "npm run --silent test:watch -- --coverage",
    "test:coverage": "true",
    "lint": "eslint --fix examples src",
    "docs:build": "npm run --silent docs:jsdoc:build && npm run --silent docs:storybook:build && npm run --silent docs:getting-started:build",
    "docs:jsdoc:build": "jsdoc -c ../../jsdoc.json",
    "docs:storybook:build": "build-storybook --output-dir ../../docs/packages/$npm_package_name/$npm_package_version/examples",
    "docs:storybook:watch": "start-storybook -p 9001",
    "docs:getting-started:build": "NODE_ENV=production SKIP_PREFLIGHT_CHECK=true PUBLIC_URL=./ react-scripts build && mv build ../../docs/packages/$npm_package_name/$npm_package_version/getting-started"
  },
  "author": "Francois-Guillaume Ribreau <npm@fgribreau.com> (http://fgribreau.com/)",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "4.1.16",
    "@storybook/addon-backgrounds": "4.1.16",
    "@storybook/addon-centered": "4.1.16",
    "@storybook/addon-jest": "4.1.16",
    "@storybook/addon-knobs": "4.1.16",
    "@storybook/addon-links": "4.1.16",
    "@storybook/addon-notes": "4.1.16",
    "@storybook/addon-options": "4.1.16",
    "@storybook/addon-storyshots": "4.1.16",
    "@storybook/addon-storysource": "4.1.16",
    "@storybook/addon-viewport": "4.1.16",
    "@storybook/addons": "4.1.16",
    "@storybook/react": "4.1.16",
    "jsdoc": "^3.5.5",
    "react-scripts": "^2.1.8"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "dependencies": {
    "ui-predicate-core": "^0.6.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
