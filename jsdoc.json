{
  "source": {
    "include": ["src", "package.json", "README.md"]
  },
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "../../docs/packages",
    "recurse": true
  },
  "docdash": {
    "home": "ui-predicate",
    "scripts": ["../../docs/docdash-overrides.scss"],
    "menu": {
      "Github": {
        "href":"https://github.com/fgribreau/ui-predicate/",
        "target":"_blank",
        "class":"menu-item",
        "id":"github_link"
      }
    }
  }
}
